from bs4 import BeautifulSoup
import requests

url = 'https://www.lueftner-cruises.com/en/river-cruises/cruise/show/classical-rhine-cruise-2019.html'

headers = {
    'User-Agent': 'Mozilla/5.0',
}


response = requests.get(url, headers=headers)

data = response.text
soup = BeautifulSoup(data, 'lxml')

first_panel = soup.find('div', attrs={'role' : 'tablist'})
print(first_panel)

